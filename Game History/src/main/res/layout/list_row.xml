<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:outlineProvider="bounds">

    <ImageView
            android:layout_width="10dp"
            android:layout_height="70dp"
            android:id="@+id/win"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:background="@android:color/holo_green_dark" android:padding="2dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/champName"
            android:gravity="center_horizontal"
            android:layout_alignParentTop="true" android:layout_alignStart="@+id/championImage"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/ss1"
            android:src="@drawable/flash" android:layout_alignParentTop="true" android:layout_toEndOf="@+id/win"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/ss2"
            android:layout_alignBottom="@+id/win"
            android:layout_toEndOf="@+id/win"
            android:src="@drawable/flash"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/level"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="New Text"
            android:id="@+id/cs"
            android:layout_alignTop="@+id/championImage" android:layout_toStartOf="@+id/item1"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/item1"
            android:layout_marginStart="16dp"
            android:src="@drawable/boots"
            android:layout_alignParentTop="true" android:layout_toEndOf="@+id/level"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/item2"
            android:src="@drawable/boots"
            android:layout_marginStart="7dp" android:layout_alignParentTop="true" android:layout_toEndOf="@+id/item1"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/item3"
            android:src="@drawable/boots" android:layout_alignParentTop="true" android:layout_toEndOf="@+id/item2"
            android:layout_marginStart="6dp"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/item4"
            android:src="@drawable/boots"
            android:layout_below="@+id/cs" android:layout_alignStart="@+id/item1"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/item5"
            android:src="@drawable/boots"
            android:layout_alignTop="@+id/item4" android:layout_toStartOf="@+id/item3"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/trinket"
            android:src="@drawable/boots"
            android:layout_below="@+id/level" android:layout_toEndOf="@+id/item3"/>
    <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:id="@+id/item6"
            android:src="@drawable/boots"
            android:layout_alignTop="@+id/item5" android:layout_alignStart="@+id/item3"/>
    <ImageView
            android:src="@drawable/boots"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/championImage"
            android:layout_alignBottom="@+id/ss2" android:layout_toEndOf="@+id/ss1" android:layout_marginStart="17dp"/>


</RelativeLayout>
